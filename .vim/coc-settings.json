{
  "suggest.noselect": false,
  "python.jediEnabled": false,
  "suggest.maxCompleteItemCount": 7,
  "suggest.detailField": "abbr",
  "suggest.floatEnable": false,
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "html",
    "javascript",
    "javascriptreact",
    "json",
    "typescript",
    "typescriptreact",
    "typescript.tsx",
    "python"
  ],
  "coc.preferences.extensionUpdateCheck": "daily",
  "emmet.includeLanguages": { "javascript": "javascriptreact" },
  "coc.github.enable": {
    "type": "boolean",
    "default": true
  },
  "coc.github.priority": {
    "type": "number",
    "default": 99
  },
  "coc.github.filetypes": {
    "type": "array",
    "default": ["gitcommit"]
  }
}
